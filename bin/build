#! /usr/bin/env bash
mkdir -p dst/songs
./bin/build-json ../songs/*.md > dst/songs.json
./bin/build-html-collection songs ./templates/basic.ejs dst/songs.json

mkdir -p dst/posts
./bin/build-json ../posts/*.md > dst/posts.json
./bin/build-html-collection posts ./templates/basic.ejs dst/posts.json

./bin/build-html homepage ./templates/basic.ejs dst/index.html
